apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  annotations:
    kubernetes.io/ingress.class: traefik
  name: radarr
  namespace: media
spec:
  rules:
  - host: radarr.kube.home
    http:
      paths:
      - backend:
          service:
            name: radarr-service
            port:
              number: 8989
        path: /
        pathType: Prefix
